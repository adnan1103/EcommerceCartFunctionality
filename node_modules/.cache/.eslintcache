bmit && cartModalContent}\r\n      {isSubmitting && isSubmittingModalContent}\r\n      {!isSubmitting && didSubmit && didSubmitModalContent}\r\n    </Modal>\r\n  );\r\n};\r\n\r\nexport default Cart;\r\n","D:\\ProjectInNexusBarry\\EcommerceCartFunctionality\\src\\components\\Layout\\Header.js",[],[],"D:\\ProjectInNexusBarry\\EcommerceCartFunctionality\\src\\components\\Meals\\Meals.js",[],[],"D:\\ProjectInNexusBarry\\EcommerceCartFunctionality\\src\\store\\cart-context.js",[],[],"D:\\ProjectInNexusBarry\\EcommerceCartFunctionality\\src\\components\\Cart\\CartItem.js",[],[],"D:\\ProjectInNexusBarry\\EcommerceCartFunctionality\\src\\components\\Meals\\MealsSummary.js",[],[],"D:\\ProjectInNexusBarry\\EcommerceCartFunctionality\\src\\components\\Meals\\AvailableMeals.js",[],[],[],"D:\\ProjectInNexusBarry\\EcommerceCartFunctionality\\src\\components\\Layout\\HeaderCartButton.js",[],[],"D:\\ProjectInNexusBarry\\EcommerceCartFunctionality\\src\\components\\UI\\Modal.js",[],[],"D:\\ProjectInNexusBarry\\EcommerceCartFunctionality\\src\\components\\Cart\\CartIcon.js",[],[],"D:\\ProjectInNexusBarry\\EcommerceCartFunctionality\\src\\components\\UI\\Card.js",[],[],"D:\\ProjectInNexusBarry\\EcommerceCartFunctionality\\src\\components\\Meals\\MealItem\\MealItem.js",[],[],"D:\\ProjectInNexusBarry\\EcommerceCartFunctionality\\src\\components\\Meals\\MealItem\\MealItemForm.js",[],[],"D:\\ProjectInNexusBarry\\EcommerceCartFunctionality\\src\\components\\UI\\Input.js",[],[],"D:\\ProjectInNexusBarry\\EcommerceCartFunctionality\\src\\components\\Cart\\Checkout.js",[],[],{"ruleId":"97","severity":1,"message":"98","line":1,"column":17,"nodeType":"99","messageId":"100","endLine":1,"endColumn":25},{"ruleId":"97","severity":1,"message":"101","line":1,"column":8,"nodeType":"99","messageId":"100","endLine":1,"endColumn":13},"no-unused-vars","'Fragment' is defined but never used.","Identifier","unusedVar","'Raect' is defined but never used."]